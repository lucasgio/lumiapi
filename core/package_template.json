{
  "name": "restsnap",
  "version": "1.0.0",
  "description": "A fast and intuitive way to start a REST API project with Node.js, using a modular and zero-dependency approach.",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/app.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prettier-check": "prettier --check ."
  },
  "keywords": ["typescript", "node", "starter"],
  "author": "Iosvany Alvarez",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
